{
    "name": "42videobricks/php-client",
    "version": "1.2.1",
    "description": "42videobricks is a Video Platform As A Service (VPaaS)",
    "keywords": [
        "42videobricks",
        "video",
        "api",
        "php",
        "sdk",
        "rest"
    ],
    "homepage": "https://www.42videobricks.com",
    "license": "MIT",
    "authors": [
        {
            "name": "42videobricks",
            "homepage": "https://www.42videobricks.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Api42Vb\\Client\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Api42Vb\\Client\\Test\\" : "test/" }
    }
}
